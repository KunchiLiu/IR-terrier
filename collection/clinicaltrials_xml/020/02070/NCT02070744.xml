  <primary_outcome>
    <measure>Safety as determined by adverse events (AEs), physical examination, clinical laboratory values, standard digital electrocardiograms (ECGs), vital signs and pulse oximetry</measure>
    <time_frame>baseline through Week 16</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Absolute change in percent predicted forced expiratory volume in 1 second (FEV1)</measure>
    <time_frame>baseline through Week 16</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Relative change in percent predicted FEV1</measure>
    <time_frame>from baseline through Week 12</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Absolute change in body weight</measure>
    <time_frame>From baseline at Week 12</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Absolute change in body mass index (BMI)</measure>
    <time_frame>From baseline at Week 12</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Absolute change in the respiratory domain of the Cystic Fibrosis Questionnaire-Revised (CFQ-R) Questionnaire-Revised (CFQ-R)</measure>
    <time_frame>baseline through Week 12</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>PK parameters estimates of VX-661 and ivacaftor and their respective metabolites, derived from plasma concentration-time data</measure>
    <time_frame>Day 1 through Week 16</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Absolute change in sweat chloride</measure>
    <time_frame>from baseline through Week 12</time_frame>
  </secondary_outcome>
