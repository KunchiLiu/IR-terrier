  <brief_title>Long-term Follow-up Study of GLPG0634 in Active Rheumatoid Arthritis Patients</brief_title>
  <acronym>DARWIN3</acronym>
  <official_title>A Multicenter, Open-label, Long-term Follow-up Safety and Efficacy Study of GLPG0634 Treatment in Subjects With Moderately to Severely Active Rheumatoid Arthritis</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Gilead Sciences</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
    <collaborator>
      <agency>Galapagos NV</agency>
      <agency_class>Industry</agency_class>
    </collaborator>
  </sponsors>
