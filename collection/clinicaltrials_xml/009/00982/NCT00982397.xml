  <primary_outcome>
    <measure>Percentage of subjects with unanticipated severe adverse device effects</measure>
    <time_frame>Implant to one month post-implant</time_frame>
    <description>Primary objective of Phase I</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Percentage of subjects where the ventricular fibrillation (VF) detection time with Protecta features on is no more than 2 seconds longer than the VF detection time with Protecta features off</measure>
    <time_frame>At implant</time_frame>
    <description>Primary objective of Phase I</description>
  </primary_outcome>
  <primary_outcome>
    <measure>Percentage of subjects who are inappropriate shock free</measure>
    <time_frame>Implant to one year post-implant</time_frame>
    <description>Primary objective of Phase II. Subjects implanted with a VR device will be analyzed separately from subjects implanted with a DR / CRT-D device.</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Percentage of secondary prevention subjects who are syncopal event free</measure>
    <time_frame>Implant to one year post-implant</time_frame>
  </secondary_outcome>
