    <masking>Open Label</masking>
  </study_design_info>
  <primary_outcome>
    <measure>Pharmacokinetic bioequivalence</measure>
    <time_frame>60 days</time_frame>
    <description>PK parameters AUClast and Cmax for brodalumab</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Safety and Tolerability</measure>
    <time_frame>60 days</time_frame>
    <description>Subject incidence of adverse events, vital signs, clinical laboratory tests, and standard of care electrocardiograms</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Immunogenicity</measure>
    <time_frame>60 days</time_frame>
    <description>Presence of binding or neutralizing anti-brodalumab antibodies</description>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Other PK parameters</measure>
    <time_frame>60 days</time_frame>
    <description>Other brodalumab PK parameters including AUCinf and tmax</description>
  </secondary_outcome>
