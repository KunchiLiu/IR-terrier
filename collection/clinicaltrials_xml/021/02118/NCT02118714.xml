  <brief_title>Atrasentan Spermatogenesis and Testicular Function</brief_title>
  <official_title>A Multicenter, Single-Arm Study of the Effects of Atrasentan on Spermatogenesis and Testicular Function</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>AbbVie</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>AbbVie</source>
  <oversight_info>
