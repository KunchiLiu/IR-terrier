    <time_frame>Week 17-20</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in serum phosphorus from baseline to the mean value during weeks 17-20</measure>
    <time_frame>Week 17-20</time_frame>
  </secondary_outcome>
  <secondary_outcome>
