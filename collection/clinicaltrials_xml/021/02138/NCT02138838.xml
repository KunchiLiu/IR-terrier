  <primary_outcome>
    <measure>Proportion of subjects who achieve a ≥ 30% reduction from baseline in mean plasma iPTH during Weeks 17 - 20</measure>
    <time_frame>Week 17-20</time_frame>
  </primary_outcome>
  <primary_outcome>
    <measure>US only proportion of subjects who achieve a ≥ 30% reduction from baseline in mean iPTH at Weeks 11 &amp; 15</measure>
    <time_frame>Weeks 11&amp;15</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Proportion of subjects who achieve a mean iPTH ≤ 300 pg/mL (31.8 pmol/L) during weeks 17-20</measure>
    <time_frame>Week 17-20</time_frame>
  </secondary_outcome>
  <secondary_outcome>
