  <brief_title>Acute Effects of Liothyronine in Single Administration</brief_title>
  <official_title>Effects of Liothyronine on Energy Expenditure and Cardiovascular Function</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Virginia Commonwealth University</agency>
      <agency_class>Other</agency_class>
