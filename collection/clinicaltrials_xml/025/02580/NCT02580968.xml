  <brief_title>Study of Key Electro-acupuncture Technique on Migraine</brief_title>
  <official_title>Study of Key Electro-acupuncture Technique on Migraine</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Shanghai University of Traditional Chinese Medicine</agency>
