  </primary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Anticipated">50</enrollment>
  <condition>Lower Limb Amputation at Ankle (Injury)</condition>
  <arm_group>
    <arm_group_label>Propriofoot prosthesis</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
