  <brief_title>The Study to Reevaluate the Safety and Efficacy of Human Urinary Kallikrein</brief_title>
  <official_title>The Study to Reevaluate the Safety and Efficacy of Human Urinary Kallikrein</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Techpool Bio-Pharma Co., Ltd.</agency>
