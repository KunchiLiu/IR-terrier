  <start_date>October 2015</start_date>
  <completion_date type="Anticipated">April 2019</completion_date>
  <primary_completion_date type="Anticipated">June 2018</primary_completion_date>
  <phase>Phase 3</phase>
  <study_type>Interventional</study_type>
