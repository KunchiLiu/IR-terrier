  <brief_title>Impact of Exercise Training on Pain and Brain Function in Gulf War Veterans</brief_title>
  <official_title>Impact of Exercise Training on Pain and Brain Function in Gulf War Veterans</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>VA Office of Research and Development</agency>
      <agency_class>U.S. Fed</agency_class>
    </lead_sponsor>
  </sponsors>
