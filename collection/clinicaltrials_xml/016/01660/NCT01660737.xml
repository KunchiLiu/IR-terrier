  <brief_title>Observational Study With PASCALLERG ® in Patients With Hay Fever</brief_title>
  <official_title>Observational Study With PASCALLERG ® in Patients With Hay Fever</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Pascoe Pharmazeutische Praeparate GmbH</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
