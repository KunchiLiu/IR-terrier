  <brief_title>Reduction of Obesity-Associated Intestinal Inflammation by Low-Fat Dairy Yogurt</brief_title>
  <official_title>Reduction of Obesity-Associated Intestinal Inflammation by Low-Fat Dairy Yogurt</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>University of Wisconsin, Madison</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
