  <primary_outcome>
    <measure>Change from baseline in the frequency of headache days</measure>
    <time_frame>Baseline, 12 Weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change from baseline in the frequency of severe headache days</measure>
    <time_frame>Baseline, 12 Weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change from baseline in the total cumulative hours of headache on headache days</measure>
    <time_frame>Baseline, 12 Weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Percentage of patients with ≥ 50% decrease from baseline in the frequency of headache days</measure>
    <time_frame>Baseline, 12 Weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Percentage of patients who are prescribed oral rescue migraine prophylactic treatment</measure>
    <time_frame>12 Weeks</time_frame>
  </secondary_outcome>
