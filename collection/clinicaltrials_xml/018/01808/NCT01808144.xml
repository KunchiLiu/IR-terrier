  </study_design_info>
  <primary_outcome>
    <measure>Proportion of subjects with an sUA level that is &lt; 5.0 mg/dL</measure>
    <time_frame>Up to approximately 2.5 years</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Resolution of at least 1 target tophi</measure>
    <time_frame>Up to Month 12</time_frame>
  </secondary_outcome>
  <number_of_arms>2</number_of_arms>
  <enrollment type="Actual">196</enrollment>
