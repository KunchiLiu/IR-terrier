    <measure>Pharmacokinetics Median time to response</measure>
    <time_frame>time to response</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Median duration of response</measure>
    <time_frame>duration of response</time_frame>
  </secondary_outcome>
