  <eligibility>
    <study_pop>
      <textblock>
        770 women undergoing ICSI cycles
      </textblock>
    </study_pop>
    <sampling_method>Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Patient age:25-40 years old

          -  BMI:18-30

          -  patients with first or second ICSI trial

        Exclusion criteria:

          -  AMH &lt;1

          -  severe male factor(total motile sperm &lt;1 million)

          -  frozen embryo transfer cycles

          -  patients with endocrinopathies or thrombophilia

          -  patients on medical treatment which may interfere with results

          -  hydrosalpinx
      </textblock>
    </criteria>
    <gender>Female</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>39 Years</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
