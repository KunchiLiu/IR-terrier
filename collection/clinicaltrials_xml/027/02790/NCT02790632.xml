  <enrollment type="Anticipated">374</enrollment>
  <condition>Subarachnoid Hemorrhage, Aneurysmal</condition>
  <arm_group>
    <arm_group_label>EG-1962 Group</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
    <description>1 dose of intraventricular EG-1962 (nimodipine microparticles) 600 mg
Up to 21 days of placebo capsules/tablets</description>
  </arm_group>
