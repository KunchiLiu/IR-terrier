  <brief_title>Volatile Anesthetic Protection Of Renal Transplants 2</brief_title>
  <acronym>VAPOR-2</acronym>
  <official_title>Volatile Anesthetic Protection Of Renal Transplants 2</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Gertrude J. Nieuwenhuijs-Moeke</agency>
