  <brief_title>PARADIGHM A Natural History Registry for Patients With Chronic Hypoparathyroidism</brief_title>
  <acronym>PARADIGHM</acronym>
  <official_title>PARADIGHM (Physicians Advancing Disease Knowledge in Hypoparathyroidism): A Natural History Registry for Patients With Chronic Hypoparathyroidism</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Shire</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
