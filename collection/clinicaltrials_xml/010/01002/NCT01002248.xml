  <condition>Multiple Myeloma</condition>
  <arm_group>
    <arm_group_label>Perifosine + Bortezomib + Dexamethasone</arm_group_label>
    <arm_group_type>Experimental</arm_group_type>
  </arm_group>
  <arm_group>
