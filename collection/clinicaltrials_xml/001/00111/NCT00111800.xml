  <primary_outcome>
    <measure>Mean change from baseline (Week 0) in HbA1c (Glycosylated haemoglobin) at Week 12.</measure>
    <time_frame>Baseline (Week 0) and Week 12</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Mean change from baseline (Week 0) in HbA1c at Week 4, 8, 16, 20 and 24</measure>
    <time_frame>Baseline (Week 0), Week 4 and Week 8</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Mean change from baseline (Week 0) in fasting plasma glucose (FPG) at Week 12</measure>
    <time_frame>Baseline (Week 0) and Week 12</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Mean change from baseline (Week 0) in fasting plasma glucose (FPG) at Week 1, 2, 3, 4, 6, 8, 13, 14, 15, 16, 20 and 24</measure>
