  <brief_title>Nutrition and Cognitive Function in Preadolescents</brief_title>
  <official_title>Nutrition and Cognitive Function in Preadolescents</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Arkansas Children's Hospital Research Institute</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
