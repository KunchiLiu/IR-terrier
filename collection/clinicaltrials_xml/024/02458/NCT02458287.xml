  <eligibility>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Treated with a statin - Fasting LDL-C &gt;=70mg/dL and triglycerides &lt;=400mg/dL

        Exclusion Criteria:

          -  Pregnant or breastfeeding females - Cardiovascular or cerebrovascular event or
             procedures during the past 90 days - Congestive heart failure NYHA class IV - Poorly
             controlled hypertension
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>18 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
