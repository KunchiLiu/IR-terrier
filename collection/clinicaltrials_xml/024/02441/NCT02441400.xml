  <brief_title>EndoStim Patient Registry</brief_title>
  <acronym>RESTORE</acronym>
  <official_title>EndoStim Patient Registry-LES Stimulation System for the Treatment of GERD</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>EndoStim Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
  <source>EndoStim Inc.</source>
  <oversight_info>
    <has_dmc>Yes</has_dmc>
  </oversight_info>
  <brief_summary>
    <textblock>
      To provide a research tool enabling physicians to monitor long term clinical outcomes for
      patients undergoing gastroesophageal reflux disease (GERD) treatment with EndoStim LES
      Stimulation System.
    </textblock>
  </brief_summary>
  <overall_status>Recruiting</overall_status>
  <start_date>May 2013</start_date>
  <completion_date type="Anticipated">May 2019</completion_date>
  <primary_completion_date type="Anticipated">May 2019</primary_completion_date>
  <phase>N/A</phase>
  <study_type>Observational [Patient Registry]</study_type>
  <has_expanded_access>No</has_expanded_access>
  <study_design_info>
    <observational_model>Cohort</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <target_duration>5 Years</target_duration>
  <primary_outcome>
    <measure>Safety will be assessed by incidence and severity of adverse events through 60-month (5 years) follow-up.</measure>
    <time_frame>5 years post implant</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change in GERD-HRQL (vs. baseline)</measure>
    <time_frame>5 years post-procedure.</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Comparisons of pH values (vs. baseline)</measure>
    <time_frame>5 years post-procedure</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Comparison of symptoms (measured by patient symptom diary) and patients quality of life measured by SF-12 (vs. baseline)</measure>
    <time_frame>5 years post-procedure</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in antisecretory medication use (vs. baseline)</measure>
    <time_frame>5 years post-procedure</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in structured GI Questionnaire responses (vs. baseline)</measure>
    <time_frame>5 years post-procedure</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in sleep related quality of life as measured by the Pittsburgh Sleep Quality Index (PSQI) (vs. baseline)</measure>
    <time_frame>5 years post-procedure</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>g. Change in work productivity impairment (on and off PPI periods) as measured by the Work Productivity and Activity Impairment Questionnaire: Specific Health Problem V2.0 (WPAI:SHP) (vs. baseline)</measure>
    <time_frame>5 years post-procedure</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Change in RDQ scores (vs. baseline)</measure>
    <time_frame>5 years post-procedure</time_frame>
  </secondary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Anticipated">350</enrollment>
  <condition>GERD</condition>
  <arm_group>
    <arm_group_label>EndoStim LES Stimulation System implant.</arm_group_label>
    <description>Registry subjects whom have been implanted commercially with the EndoStim LES Stimulation System device.</description>
  </arm_group>
  <intervention>
    <intervention_type>Device</intervention_type>
    <intervention_name>EndoStim LES Stimulation System</intervention_name>
    <arm_group_label>EndoStim LES Stimulation System implant.</arm_group_label>
  </intervention>
  <eligibility>
    <study_pop>
      <textblock>
        Subjects with a diagnosis of GERD who are not contraindicated to implantation of the
        EndoStim LES Stimulation System.
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  Subject signs informed consent

          -  Subject has a planned EndoStim LES Stimulation System implant (prospective) OR
             subject has already had his/her EndoStim LES Stimulation System implant
             (retrospective)

        Exclusion Criteria:

          -  Subject is contraindicated for implantation of the EndoStim LES Stimulation System
             implant

               -  Significant cardiac arrhythmia, ectopy, or significant cardiovascular disease

               -  Pregnant or nursing

          -  Subject is less than 22 years old or greater than 75 years old
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>21 Years</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
