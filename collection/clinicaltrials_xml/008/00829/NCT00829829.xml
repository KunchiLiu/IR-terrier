    <time_frame>16 Weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>proportion of subjects with at least two flares from Day 1 to Week 16</measure>
    <time_frame>16 Weeks</time_frame>
  </secondary_outcome>
  <secondary_outcome>
