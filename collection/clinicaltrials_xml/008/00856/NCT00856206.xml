  <primary_outcome>
    <measure>Safety assessed by summarizing the incidences and types of treatment emergent adverse events and lab parameter changes.</measure>
    <time_frame>16 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Number of gout flares from Day 1 to Week 16</measure>
    <time_frame>16 weeks</time_frame>
  </secondary_outcome>
