  <primary_outcome>
    <measure>To assess the number of ventricular and atrial arrhythmias, and atrial fibrillation burden detected by ICD.</measure>
    <time_frame>End of the study, an expected duration of 5 years.</time_frame>
  </primary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Anticipated">10000</enrollment>
