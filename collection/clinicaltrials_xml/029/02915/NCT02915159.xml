  <primary_outcome>
    <measure>Change from baseline in EULAR Sjögren's Syndrome Disease Activity Index (ESSDAI)</measure>
    <time_frame>Day 1 to Day 169</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change from baseline in EULAR Sjögren's Syndrome Patient Reported Index (ESSPRI)</measure>
    <time_frame>Day 1 to Day 169</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Mean change from baseline in the stimulated whole salivary flow among subjects with stimulated whole salivary flow of at least 0.1 mL/min at both screening and baseline</measure>
    <time_frame>Day 1 to Day 169</time_frame>
  </secondary_outcome>
  <secondary_outcome>
    <measure>Proportion of subjects with a least one positive immunogenicity response as measured by anti-abatacept antibody testing</measure>
    <time_frame>Up to 1 year</time_frame>
