  <primary_outcome>
    <measure>Incidence and exposure-time adjusted incidence rate of adverse drug reactions</measure>
    <time_frame>up to 52 weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>COPD assessment test (CAT) at 12 weeks.</measure>
    <time_frame>up to 12 weeks</time_frame>
  </secondary_outcome>
