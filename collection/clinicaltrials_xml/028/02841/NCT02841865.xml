  <brief_title>Evaluation of Radiological Response of Pancreatic Neuroendocrine Tumors: Comparison Between the Choi and the RECIST Criteria</brief_title>
  <acronym>CRIPNET</acronym>
  <official_title>Evaluation of Radiological Response of Pancreatic Neuroendocrine Tumors: Comparison Between the Choi and the RECIST Criteria</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>Grupo Espanol de Tumores Neuroendocrinos</agency>
      <agency_class>Other</agency_class>
    </lead_sponsor>
  </sponsors>
