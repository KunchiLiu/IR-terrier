  <study_design_info>
    <observational_model>Cohort</observational_model>
    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <primary_outcome>
    <measure>All reported adverse events in patients who take at least one dose of JARDIANCE速 will be noted.</measure>
    <time_frame>24weeks</time_frame>
  </primary_outcome>
  <secondary_outcome>
    <measure>Change from baseline in HbA1c after 12 weeks and/or 24 weeks of treatment</measure>
    <time_frame>24weeks</time_frame>
  </secondary_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Anticipated">3000</enrollment>
  <condition>Diabetes Mellitus, Type 2</condition>
  <arm_group>
    <arm_group_label>JARDIANCE</arm_group_label>
