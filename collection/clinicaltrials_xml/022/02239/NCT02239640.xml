    <time_perspective>Prospective</time_perspective>
  </study_design_info>
  <target_duration>90 Days</target_duration>
  <primary_outcome>
    <measure>Status of revascularization at the end of the interventional procedure</measure>
    <time_frame>Day 0</time_frame>
    <description>Assess revascularization using Thrombolysis in Cerebral Infarction (TICI) score at the end of the procedure</description>
  </primary_outcome>
  <secondary_outcome>
    <measure>Symptomatic intracranial hemorrhage</measure>
    <time_frame>90 days</time_frame>
    <description>Evaluate the rate of participants with symptomatic intracranial hemorrhage post procedure and up to 90 days</description>
  </secondary_outcome>
  <other_outcome>
    <measure>All-cause mortality</measure>
    <time_frame>90 days</time_frame>
  </other_outcome>
  <number_of_groups>1</number_of_groups>
  <enrollment type="Anticipated">1000</enrollment>
  <condition>Ischemic Stroke</condition>
  <eligibility>
    <study_pop>
      <textblock>
        Acute Ischemic Stroke Patients
      </textblock>
    </study_pop>
    <sampling_method>Non-Probability Sample</sampling_method>
    <criteria>
      <textblock>
        Inclusion Criteria:

          -  An informed consent or waiver

          -  Patient has experienced an acute ischemic stroke

          -  Treatment within 8 hours of stroke onset

        Exclusion Criteria:

          -  Concurrent participation in a randomized study
      </textblock>
    </criteria>
    <gender>All</gender>
    <minimum_age>N/A</minimum_age>
    <maximum_age>N/A</maximum_age>
    <healthy_volunteers>No</healthy_volunteers>
  </eligibility>
