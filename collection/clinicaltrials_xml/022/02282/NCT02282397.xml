  <brief_title>Multiple Daily Injections and Continuous Glucose Monitoring in Diabetes</brief_title>
  <acronym>DIaMonD</acronym>
  <official_title>Multiple Daily Injections and Continuous Glucose Monitoring in Diabetes</official_title>
  <sponsors>
    <lead_sponsor>
      <agency>DexCom, Inc.</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
