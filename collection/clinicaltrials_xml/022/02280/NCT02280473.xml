  <brief_title>A Safety and Efficacy Study of a New Eye Drop Formulation in Patients With Dry Eye Disease</brief_title>
  <sponsors>
    <lead_sponsor>
      <agency>Allergan</agency>
      <agency_class>Industry</agency_class>
    </lead_sponsor>
  </sponsors>
